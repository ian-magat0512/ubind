-- {1} copy rows {2} and {3} to each other when a row is inserted
CREATE OR ALTER TRIGGER {0}
   ON dbo.{1}
   AFTER INSERT
AS
BEGIN
    SET NOCOUNT ON;
 
    BEGIN
        UPDATE dbo.{1}
        SET {3} = i.{2}
        FROM INSERTED i
        WHERE dbo.{1}.{5} = i.{5}
        AND i.{2} {4}
    END
    BEGIN
        UPDATE dbo.{1}
        SET {2} = i.{3}
        FROM INSERTED i
        WHERE dbo.{1}.{5} = i.{5}
        AND i.{3} {4}
    END
END